{
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "printWidth": 180,
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": false,
        "bracketSpacing": false,
        "explicitTypes": "preserve"
      }
    },
    {
      "files": ["*.js", "*.ts"],
      "options": {
        "printWidth": 140,
        "semi": false,
        "singleQuote": true,
        "bracketSpacing": true,
        "trailingComma": "es5"
      }
    }
  ]
}
